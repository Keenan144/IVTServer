<p id="notice"><%= notice %></p>


<div class="container-fluid">

    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                Listing Units
                <small>Recent Units</small>
            </h1>
            <ol class="breadcrumb">
                <li>
                    <i class="fa fa-dashboard"></i>  <a href="/dashboard">Dashboard</a>
                </li>
                <li class="active">
                    <i class="fa fa-file"></i> Recent Units
                </li>
            </ol>
        </div>
    </div>
    <!-- /.row -->

</div>
<!-- /.container-fluid -->

<div class="container-fluid">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title"><i class="fa fa-money fa-fw"></i> Last Unit Locations</h3>
        </div>
        <div class="panel-body">
            <div class="table-responsive">
                <table class="table table-bordered table-hover table-striped">
                    <thead>
                        <tr>
                            <th>Unit Number</th>
                            <th>Updated At</th>
                            <th>User</th>
                            <th>Address</th>
                            <th>Flag</th>
                        </tr>
                    </thead>
                    <% @units.each do |unit| %>
                      <tr>
                        <td><strong> <%= link_to unit.unit_number, unit %></strong></td>
                        <td> <%= link_to unit.updated_at, unit %></td>
                        <td> <%= link_to unit.username, unit %></td>
                        <td> <%= link_to unit.address, unit %></td>
                        <td><%= link_to 'Edit', edit_unit_path(unit) %></td>

                      </tr>
                    <% end %>
                    <tbody>
  
                    </tbody>
                </table>
            </div>
            <div class="text-right">
                <a href="#">View All Transactions <i class="fa fa-arrow-circle-right"></i></a>
            </div>
        </div>
    </div>
</div>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  </tbody>
</table>

